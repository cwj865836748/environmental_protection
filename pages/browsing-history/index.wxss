/* pages/browsing-history/index.wxss */
@import '/pages/my-collect/index.wxss';
@import '/pages/innovative-technology/index.wxss';
@import '/common/enterprise/enterpriseList.wxss';
.history-wrap{
  background-color: #ffffff;
  position: relative;
}
.bg-change{
  width: 100%;
  height: 256rpx;
  position: absolute;
  top: 0;
  left: 0;
  background-image: linear-gradient(180deg, #ffffff, rgba(255, 255, 255, 0));
  z-index: -1;
}
.news-content {
  background-color: #f8f8f8;
  padding-bottom: 20rpx;
  overflow-y: scroll;
}
.news-content1 {
  padding-bottom: 20rpx;
  overflow-y: scroll;
}

.tab-lists {
  width: 100%;
  background-image: linear-gradient(180deg, #ffffff, rgba(255, 255, 255, 0));
  padding:30rpx 0 30rpx;
}
.shop-list{
  width: 690rpx;
  margin: 0 auto;
  background-color: #ffffff;
  margin-top: 30rpx;
  margin-bottom: 0;
}
